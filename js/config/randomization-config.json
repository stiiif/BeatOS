{
  "_meta": {
    "version": "1.2.0",
    "description": "BeatOS Randomization Configuration â€” controls which parameters the 'Rand Prms' button affects",
    "modes": {
      "randomize": "Parameter will be randomized within min/max range (or snapValues if present)",
      "skip": "Parameter is ignored entirely by randomization",
      "lock": "Parameter is actively written back to its current value (protects from automation drift)"
    },
    "curves": {
      "uniform": "Equal probability across range",
      "gaussian": "Bell curve centered between min/max (good for pitch, pan)",
      "exponential": "Biased toward min value (good for chaos/spray params)",
      "inverseExponential": "Biased toward max value (good for filter cutoff)"
    },
    "scopes": {
      "all": "Apply to all tracks/instances in this group",
      "selected": "Apply only to the currently selected track",
      "indices": "Apply only to specific indices listed in scopeIndices"
    },
    "zonedExplanation": "When 'zoned' is true on a parameter, the Rand Prms button click position (left=zone 1, right=last zone) scales the effective randomization range. Zone 1 uses a sub-range near min, last zone uses a sub-range near max. The zones below define the named breakpoints and their label shown on the button. For non-zoned params, the full min/max range is always used regardless of click position.",
    "zones": [
      { "name": "VERY SHORT" },
      { "name": "SHORT" },
      { "name": "MEDIUM" },
      { "name": "LONG" },
      { "name": "VERY LONG" }
    ]
  },

  "granularEngine": {
    "_label": "Granular Synthesis Engine",
    "_scope": "all",
    "_scopeIndices": [],
    "params": {
      "position":   { "mode": "randomize", "min": 0.0,   "max": 1.0,   "chance": 1.0, "curve": "uniform",     "snapValues": null, "zoned": false },
      "spray":      { "mode": "randomize", "min": 0.0,   "max": 0.1,   "chance": 0.8, "curve": "exponential", "snapValues": null, "zoned": false },
      "scanSpeed":  { "mode": "skip",      "min": 0.0,   "max": 0.5,   "chance": 0.5, "curve": "exponential", "snapValues": null, "zoned": false },
      "density":    { "mode": "skip",      "min": 1,     "max": 80,    "chance": 0.7, "curve": "uniform",     "snapValues": [5, 10, 20, 40, 60, 80], "zoned": false },
      "overlap":    { "mode": "skip",      "min": 0.0,   "max": 4.0,   "chance": 0.6, "curve": "uniform",     "snapValues": null, "zoned": false },
      "grainSize":  { "mode": "randomize", "min": 0.05,  "max": 0.3,   "chance": 1.0, "curve": "uniform",     "snapValues": null, "zoned": false },
      "stereoSpread":{ "mode": "randomize","min": 0.0,   "max": 0.8,   "chance": 0.6, "curve": "exponential", "snapValues": null, "zoned": true },
      "pitch":      { "mode": "skip",      "min": 0.5,   "max": 2.0,   "chance": 1.0, "curve": "gaussian",    "snapValues": [0.25, 0.5, 0.75, 1.0, 1.5, 2.0], "zoned": false },
      "pitchSemi":  { "mode": "randomize", "min": -12,   "max": 12,    "chance": 1.0, "curve": "gaussian",    "snapValues": [-24, -12, -7, -5, 0, 5, 7, 12, 24], "zoned": false },
      "pitchFine":  { "mode": "skip",      "min": -50,   "max": 50,    "chance": 0.3, "curve": "gaussian",    "snapValues": null, "zoned": false },
      "chordType":  { "mode": "skip" },
      "chordSpread":{ "mode": "skip",      "min": 0,     "max": 2,     "chance": 0.3, "curve": "uniform",     "snapValues": [0, 1, 2], "zoned": false },
      "relGrain":   { "mode": "randomize", "min": 0.01,  "max": 2.0,   "chance": 1.0, "curve": "uniform",     "snapValues": null, "zoned": true },
      "sampleStart":{ "mode": "skip",      "min": 0.0,   "max": 0.5,   "chance": 0.3, "curve": "exponential", "snapValues": null, "zoned": false },
      "sampleEnd":  { "mode": "skip",      "min": 0.5,   "max": 1.0,   "chance": 0.3, "curve": "inverseExponential", "snapValues": null, "zoned": false },
      "edgeCrunch": { "mode": "skip",      "min": 0.0,   "max": 0.5,   "chance": 0.3, "curve": "exponential", "snapValues": null, "zoned": false },
      "orbit":      { "mode": "skip",      "min": 0.0,   "max": 0.5,   "chance": 0.3, "curve": "exponential", "snapValues": null, "zoned": false },
      "ampAttack":  { "mode": "skip",      "min": 0.001, "max": 0.5,   "chance": 0.4, "curve": "exponential", "snapValues": null, "zoned": false },
      "ampDecay":   { "mode": "skip",      "min": 0.001, "max": 0.5,   "chance": 0.4, "curve": "exponential", "snapValues": null, "zoned": false },
      "ampRelease": { "mode": "skip",      "min": 0.001, "max": 0.5,   "chance": 0.4, "curve": "exponential", "snapValues": null, "zoned": false }
    }
  },

  "drum909": {
    "_label": "909 Drum Engine",
    "_scope": "all",
    "_scopeIndices": [],
    "params": {
      "drumTune":  { "mode": "randomize", "min": 0.0, "max": 1.0, "chance": 1.0, "curve": "uniform", "snapValues": null, "zoned": false },
      "drumDecay": { "mode": "randomize", "min": 0.0, "max": 1.0, "chance": 1.0, "curve": "uniform", "snapValues": null, "zoned": true }
    }
  },

  "modulatorLFOs": {
    "_label": "Track Modulator LFOs (per-track, 3 LFOs each)",
    "_scope": "all",
    "_scopeIndices": [],
    "perLFO": {
      "wave":          { "mode": "randomize", "min": null,  "max": null,  "chance": 0.7,  "curve": "uniform", "snapValues": ["sine", "square", "sawtooth", "triangle", "pulse", "random"], "zoned": false },
      "rate":          { "mode": "randomize", "min": 0.1,   "max": 20.0,  "chance": 0.7,  "curve": "uniform", "snapValues": null, "zoned": false },
      "amount":        { "mode": "randomize", "min": 0.1,   "max": 1.0,   "chance": 0.7,  "curve": "uniform", "snapValues": null, "zoned": false },
      "sync":          { "mode": "skip",      "min": null,  "max": null,  "chance": 0.3,  "curve": "uniform", "snapValues": [true, false], "zoned": false },
      "syncRateIndex": { "mode": "skip",      "min": 0,     "max": 29,    "chance": 0.5,  "curve": "uniform", "snapValues": [7, 9, 12, 15, 20, 24], "zoned": false },
      "targets":       { "mode": "randomize", "min": null,  "max": null,  "chance": 0.7,  "curve": "uniform", "zoned": false,
        "maxTargets": 3,
        "snapValues": ["position", "spray", "density", "grainSize", "stereoSpread", "pitch", "filter", "hpFilter", "sampleStart", "sampleEnd", "scanSpeed", "overlap", "edgeCrunch", "orbit"]
      },
      "_activationChance": 0.7
    }
  },

